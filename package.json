{
  "name": "eslint-config-zenflow",
  "version": "2.1.1",
  "description": "A standard eslint configuration",
  "main": ".eslintrc.js",
  "files": [
    ".editorconfig",
    ".eslintrc.js",
    ".gitattributes",
    "CHANGELOG.md"
  ],
  "scripts": {
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "test": "npm run eslint-check && npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zenflow/eslint-config-zenflow.git"
  },
  "author": {
    "name": "Matthew Francis Brunetti",
    "email": "zenflow87@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zenflow/eslint-config-zenflow/issues"
  },
  "homepage": "https://github.com/zenflow/eslint-config-zenflow#readme",
  "dependencies": {
    "babel-eslint": "^8.0.3",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0"
  },
  "peerDependencies": {
    "eslint": ">=4.12.1",
    "eslint-plugin-import": ">=2.8.0",
    "eslint-plugin-node": ">=6.0.1",
    "eslint-plugin-prettier": ">=2.3.1",
    "eslint-plugin-promise": ">=3.6.0",
    "eslint-plugin-react": ">=7.5.1",
    "eslint-plugin-standard": ">=3.0.1",
    "prettier": ">=1.9.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^2.0.1",
    "@semantic-release/git": "^4.0.1",
    "eslint": "^4.18.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "prettier": "^2.0.0",
    "semantic-release": "^15.0.3"
  },
  "eslintConfig": {
    "extends": [
      "./.eslintrc.js"
    ]
  },
  "eslintIgnore": [
    "node_modules/"
  ]
}
